{
  "port": 8080,
  "ip": "0.0.0.0",
  "server_name": "localhost:8080",
  "admins": [],
  "max_blob_size": 25000000,
  "root_redirect": "/dashboard",
  "default_driver": "disk",
  "drivers": {
    "udb": {
      "path": "./default-drivers/user-dropbox",
      "required": true
    },
    "disk": {
      "path": "./default-drivers/disk",
      "storage_root_directory": "../../hestia-storage",
      "read_url": "localhost:8080/disk/read",
      "max_user_size": 25000000,
      "max_shared_size": 2000000000,
      "required": false
    }
  },
  "plugins": {
    "encrypt-all": {
      "path": "./default-plugins/encrypt-all",
      "key": "keyboardcat"
    },
    "sync": {
      "path": "./default-plugins/sync"
    },
    "dashboard": {
      "path": "./default-plugins/dashboard"
    }
  }
}
